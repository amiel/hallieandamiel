<h2>Duplicate Photos</h2>


<% d = @duplicates.first %>
<% d.each do |photo| %>

  <%= image_tag photo.photo.url(:full), class: 'full_photo' %>
  <section class="metadata">

    <section class="tags">
      <ul class="tags">
      <% photo.tags.each { |tag| %>
        <li class="tag" id="<%=dom_id(tag)%>"><%= tag %></li>
      <% } %>
      </ul>
    </section>

    <dl>
      <dt>File Name</dt>
      <dd><%= photo.photo_file_name %></dd>

      <dt>Uploader name</dt>
      <dd><%= photo.uploader_name %></dd>

      <dt>Uploader ip</dt>
      <dd><%= photo.uploader_ip %></dd>

      <dt>File size</dt>
      <dd><%= photo.photo_file_size %></dd>

      <dt>Uploaded at</dt>
      <dd><%= photo.created_at %></dd>
    </dl>
  </section>
<% end %>